{
  "name": "dev",
  "description": "This is a dev environment for Hydrogen",
  "version": "2.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn build:client && yarn build:server && yarn build:worker",
    "build:client": "vite build --outDir dist/client --manifest",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.jsx",
    "build:worker": "WORKER=true vite build --outDir dist/worker --ssr worker.js",
    "serve": "node --enable-source-maps server"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@vitejs/plugin-react-refresh": "^1.3.2",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.10",
    "tailwindcss": "^2.1.1",
    "vite": "^2.3.3"
  },
  "dependencies": {
    "@shopify/h2-internal": "2.0.3",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "node-fetch": "^2.6.1",
    "react": "0.0.0-experimental-3c2341416",
    "react-dom": "0.0.0-experimental-3c2341416",
    "react-router-dom": "^5.2.0",
    "serve-static": "^1.14.1"
  }
}
