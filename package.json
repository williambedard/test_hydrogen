{
  "name": "hydrogen-app",
  "description": "This is a dev environment for Hydrogen",
  "version": "1.0.0",
  "license": "MIT",
  "private": true,
  "engines": {
    "node": ">=12.0.0"
  },
  "scripts": {
    "dev": "vite",
    "build": "yarn build:client && yarn build:server && yarn build:worker",
    "build:client": "vite build --outDir dist/client",
    "build:server": "vite build --outDir dist/server --ssr src/entry-server.jsx",
    "build:worker": "WORKER=true vite build --outDir dist/worker --ssr worker.js",
    "serve": "node --enable-source-maps server"
  },
  "devDependencies": {
    "@tailwindcss/typography": "^0.4.0",
    "@vitejs/plugin-react-refresh": "^1.3.2",
    "autoprefixer": "^10.2.5",
    "postcss": "^8.2.10",
    "tailwindcss": "^2.1.1",
    "vite": "^2.3.3"
  },
  "dependencies": {
    "@shopify/h2-internal": "^1.0.0",
    "compression": "^1.7.4",
    "express": "^4.17.1",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-router-dom": "^5.2.0",
    "serve-static": "^1.14.1",
    "@actions/core": "^1.2.2",
    "@actions/github": "^2.1.1"
  }
}
